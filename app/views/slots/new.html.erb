<div class="container center">
<h1>Slots#new</h1>

<h1>Time Slot Info</h1>
  
  <div class="row">
    <div class="col-md-8">
      <%= form_for @slot do |f| %>
        <div>
          <% f.hidden_field :approved, value: false  %>
        </div>
        <div>
        <%= f.submit "Apply for slot" %>
        </div>
        
    <% end %>
  </div>
</div>
</div>