<h1><%= @venue.name %></h1>
<h3>Event info</h3>

  <div class="row">
    <div class="col-md-8">
      <%= form_for [@venue, @event] do |f| %>
        <div>
        <%= f.label :Title %>
        <%= f.text_field :title %>
        <div>
        <%= f.label :date %>
        <%= f.date_field :date %>
        </div>
        <div>
        <%= f.label :"21+?" %>
        <%= f.check_box :twentyOne %>
        </div>
        <div>
        <%= f.label :"cover?" %>
        <%= f.check_box :cover %>
        </div>
        <div> 
        <%= f.label :stage %>
        <%= f.text_field :stage %>
        </div>
        <div>
        <%= f.label :equipment %>
        <%= f.text_field :equipment %>
        </div>
        <div>
        <%= f.label :description %>
        <%= f.text_field :description %>
        </div>
        <div>
        <%= f.label :other %>
        <%= f.text_field :other %>
        </div>
        <div> 
        <%= f.label :disclaimer %>
        <%= f.text_field :disclaimer %>
        
        </div>
        <%= f.hidden_field :venue_id, value: @venue.id %>
        <%= f.hidden_field :event_id, value: @event.id %>
        <div>

        <%= f.submit "Save" %>
        </div>
        
    <% end %>
  </div>
</div>